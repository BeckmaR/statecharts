<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.yakindu.sct.generator.core.generator">
      <SCTGenerator
            bindings="org.yakindu.sct.generator.pax.PaxCodeGeneratorModule"
            contentType="statechart"
            elementRefType="org.yakindu.sct.model.sgraph.Statechart"
            executor="org.yakindu.sct.generator.pax.GeneratorEntryExecutor1"
            id="yakindu::pax"
            name="YAKINDU SCT PAX Code Generator">
      </SCTGenerator>
   </extension>
   <extension
         point="org.yakindu.sct.generator.core.extensions">
      <ExtensionGeneratorMapping
            fileExtension="sct"></ExtensionGeneratorMapping>
   </extension>
   <extension
         point="org.yakindu.sct.generator.core.featuretypes">
      <FeatureLibrary
            defaultProvider="org.yakindu.sct.generator.pax.features.PaxDefaultFeatureValueProvider"
            library_id="org.yakindu.sct.generator.feature.pax"
            uri="platform:/plugin/org.yakindu.sct.generator.pax/library/FeatureTypeLibrary.xmi">
      </FeatureLibrary>
   </extension>

</plugin>
